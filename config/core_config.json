{
	// External binaries the program needs to be able to call. The preflight
	// checks look for these. There's no earthly reason to change them. If
	// you absolutely positively don't want any of these executed, don't use
	// this software. Prior to execution smfCommand module looks at this
	// list to make sure it's allowed to run a command.

	"external_bins": [
		"/bin/ppriv",
		"/bin/zonename",
		"/bin/svcs",
		"/bin/svcprop",
		"/usr/sbin/svcadm",
		"/usr/sbin/svccfg"
		]
}

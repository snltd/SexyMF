{
	// External binaries the program needs to be able to call. The preflight
	// checks look for these. There's no earthly reason to change them. If
	// you absolutely positively don't want any of these executed, don't use
	// this software.

	"required_bins": [
		"/bin/ppriv",
		"/bin/zonename",
		"/bin/svcs",
		"/bin/svcprop",
		"/usr/sbin/svcadm"
		],

	"listen_port": 9206,
		// The port the server listens on

	"loglines": 100,
		// Unless told otherwise, send the last "this many" lines of log
		// file

	"verbose" : true
}
